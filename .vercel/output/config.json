{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/contact-simple/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/debug-env/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/email-test/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/mail/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/test-email/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/books/?$",
			"dest": "_render"
		},
		{
			"src": "^/now/?$",
			"dest": "_render"
		},
		{
			"src": "^/timeline/?$",
			"dest": "_render"
		},
		{
			"src": "^/uses/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}